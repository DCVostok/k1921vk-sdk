                </enumeratedValue>
                <enumeratedValue>
                  <name>SyncOsc</name>
                  <description>Pseudo-Random generator with synchronous oscillators for simulation purpose</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- TESTEN: Select input for conditioning function and continuous tests: 0: Noise source (normal mode) 1: Test data register (test mode) -->
            <field>
              <name>TESTEN</name>
              <description>Select input for conditioning function and continuous tests: 0: Noise source (normal mode) 1: Test data register (test mode)</description>
              <bitRange>[2:2]</bitRange>
              <access>read-write</access>
            </field>
            <!-- CONDBYPASS: 0: Conditioning function is used (normal mode); 1: Conditioning function is bypassed (to observe entropy source directly) -->
            <field>
              <name>CONDBYPASS</name>
              <description>0: Conditioning function is used (normal mode); 1: Conditioning function is bypassed (to observe entropy source directly)</description>
              <bitRange>[3:3]</bitRange>
              <access>read-write</access>
            </field>
            <!-- TFAILINTEN: Enable interrupt if any of the health test fails -->
            <field>
              <name>TFAILINTEN</name>
              <description>Enable interrupt if any of the health test fails</description>
              <bitRange>[4:4]</bitRange>
              <access>read-write</access>
            </field>
